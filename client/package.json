{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.12.0",
    "classnames": "^2.2.5",
    "lodash": "^4.16.4",
    "moment": "^2.15.1",
    "react": "^15.3.2",
    "react-bootstrap": "^0.30.8",
    "react-cookie": "^0.4.8",
    "react-dom": "^15.3.2",
    "react-masonry-component": "^4.2.2",
    "react-medium-editor": "^1.8.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-router-bootstrap": "^0.23.1",
    "redux": "^3.6.0",
    "redux-form": "^6.1.0",
    "redux-thunk": "^2.1.0",
    "validator": "^6.0.0"
  },
  "devDependencies": {
    "cross-env": "^3.2.4",
    "eslint": "^3.18.0",
    "eslint-config-standard": "^7.1.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^2.1.1",
    "node-sass": "^4.5.0",
    "npm-run-all": "^4.0.2",
    "react-scripts": "0.9.5"
  },
  "proxy": "http://localhost:3001",
  "scripts": {
    "build-css": "node-sass src/stylesheets/style.scss -o src/stylesheets/compiled",
    "watch-css": "npm run build-css && node-sass src/stylesheets/style.scss -o src/stylesheets/compiled --watch --recursive",
    "start-js": "cross-env NODE_PATH=src react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "cross-env NODE_PATH=src react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
